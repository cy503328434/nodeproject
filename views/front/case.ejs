<!DOCTYPE html>
<html>
<head>
    <title>Home</title>
    <!-- for-mobile-apps -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta name="keywords" content=""/>
    <script type="application/x-javascript"> addEventListener("load", function () {
            setTimeout(hideURLbar, 0);
        }, false);
        function hideURLbar() {
            window.scrollTo(0, 1);
        } </script>
    <!-- //for-mobile-apps -->
    <link href="/front/css/bootstrap.css" rel="stylesheet" type="text/css" media="all"/>
    <link href="/front/css/style.css" rel="stylesheet" type="text/css" media="all"/>
    <link rel="stylesheet" href="/front/css/flexslider.css" type="text/css" media="screen" property=""/>
    <!-- js -->
    <script type="text/javascript" src="/front/js/jquery-2.1.4.min.js"></script>
    <!-- //js -->
    <!-- start-smoth-scrolling -->
    <script type="text/javascript" src="/front/js/move-top.js"></script>
    <script type="text/javascript" src="/front/js/easing.js"></script>
    <script type="text/javascript" src="/front/js/wySilder.min.js"></script>


    <style type="text/css">
        .case {
            width: 100%;
            height: auto;
            overflow: hidden;
        }

        .case li {
            list-style: none;
            float: left;
            padding: 5px;
            height: auto;
        }
        .case h4{
            text-align: center;
        }
    </style>
    <script type="text/javascript">
        jQuery(document).ready(function ($) {
            $(".scroll").click(function (event) {
                event.preventDefault();
                $('html,body').animate({scrollTop: $(this.hash).offset().top}, 1000);
            });

            $(".js-silder").silder({
                auto: true,//自动播放，传入任何可以转化为true的值都会自动轮播
                speed: 20,//轮播图运动速度
                sideCtrl: true,//是否需要侧边控制按钮
                bottomCtrl: true,//是否需要底部控制按钮
                defaultView: 0,//默认显示的索引
                interval: 2000,//自动轮播的时间，以毫秒为单位，默认3000毫秒
                activeClass: "active",//小的控制按钮激活的样式，不包括作用两边，默认active
            });
        });
    </script>
    <!-- start-smoth-scrolling -->
</head>

<body>
<!-- header -->
<div class="top-nav">
    <div class="container">
        <nav class="navbar navbar-default">
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse navbar-right" id="bs-example-navbar-collapse-1">
                <nav class="menu menu--shylock">
                    <ul class="nav navbar-nav link-effect-7" id="link-effect-7">
                        <li><a href="/" data-hover="首页">首页</a></li>
                        <li><a href="/service" data-hover="产品服务">产品服务</a></li>
                        <li class="active"><a href="/case" data-hover="客户案例">客户案例</a></li>
                        <li><a href="/knowledge" data-hover="科普知识">科普知识</a></li>
                        <li><a href="/about" data-hover="关于我们">关于我们</a></li>
                        <li><a href="contact" data-hover="联系我们">联系我们</a></li>
                    </ul>
                </nav>
            </div>
        </nav>
    </div>
</div>
<!-- //header -->
<!-- banner -->
<div class="js-silder">
    <div class="silder-scroll">
        <div class="silder-main">
            <% if(data && data.banner) {
            data.banner.forEach(function (banner) { %>
            <div class="silder-main-img">
                <img src="<%= banner.img%>" alt="<%= banner.title%>">
            </div>
            <% })
            } %>
        </div>
    </div>
</div>
<!-- //banner -->
<div>
    <div class="container">
        <h3 class="head"><span class="glyphicon" aria-hidden="true"></span>客户案列</h3>
        <div class="bs-example bs-example-tabs" role="tabpanel" data-example-id="togglable-tabs">
            <ul id="myTab" class="nav nav-tabs" role="tablist">
                <li role="presentation" class="active"><a href="#home" id="home-tab" role="tab" data-toggle="tab" aria-controls="home" aria-expanded="true">政府国企</a></li>
                <li role="presentation"><a href="#Feature1" role="tab" id="Feature1-tab" data-toggle="tab" aria-controls="Feature1">家庭客户</a></li>
                <li role="presentation"><a href="#Feature2" role="tab" id="Feature2-tab" data-toggle="tab" aria-controls="Feature2">汽车案例</a></li>
                <li role="presentation"><a href="#Feature3" role="tab" id="Feature3-tab" data-toggle="tab" aria-controls="Feature3">其他机构</a></li>
            </ul>
            <div id="myTabContent" class="tab-content">
                <div role="tabpanel" class="tab-pane fade in active" id="home" aria-labelledby="home-tab">
                    <div class="w3agile_tabs">
                        <ul class="case">
                            <% if(data && data.gov) {
                            data.gov.forEach(function (bean) { %>
                            <li>
                                <a href="/content/detail/<%= bean.id%>">
                                    <img src="<%= bean.img%>" alt=" " width="320px" height="320px"/>
                                </a>
                                <div style="margin-top: 5px;">
                                    <h4><%= bean.title%></h4>
                                </div>
                            </li>
                            <% })
                            } %>
                        </ul>
                        <div class="clearfix"> </div>
                    </div>
                </div>
                <div role="tabpanel" class="tab-pane fade" id="Feature1" aria-labelledby="Feature1-tab">
                    <div class="w3agile_tabs">
                        <ul class="case">
                            <% if(data && data.family) {
                            data.family.forEach(function (bean) { %>
                            <li>
                                <a href="/content/detail/<%= bean.id%>">
                                    <img src="<%= bean.img%>" alt=" " width="320px" height="320px"/>
                                </a>
                                <div style="margin-top: 5px;">
                                    <h4><%= bean.title%></h4>
                                </div>
                            </li>
                            <% })
                            } %>
                        </ul>
                        <div class="clearfix"> </div>
                    </div>
                </div>
                <div role="tabpanel" class="tab-pane fade" id="Feature2" aria-labelledby="Feature2-tab">
                    <div class="w3agile_tabs">
                        <ul class="case">
                            <% if(data && data.car) {
                            data.car.forEach(function (bean) { %>
                            <li>
                                <a href="/content/detail/<%= bean.id%>">
                                    <img src="<%= bean.img%>" alt=" " width="320px" height="320px"/>
                                </a>
                                <div style="margin-top: 5px;">
                                    <h4><%= bean.title%></h4>
                                </div>
                            </li>
                            <% })
                            } %>
                        </ul>
                        <div class="clearfix"> </div>
                    </div>
                </div>
                <div role="tabpanel" class="tab-pane fade" id="Feature3" aria-labelledby="Feature3-tab">
                    <div class="w3agile_tabs">
                        <ul class="case">
                            <% if(data && data.other) {
                            data.other.forEach(function (bean) { %>
                            <li>
                                <a href="/content/detail/<%= bean.id%>">
                                    <img src="<%= bean.img%>" alt=" " width="320px" height="320px"/>
                                </a>
                                <div style="margin-top: 5px;">
                                    <h4><%= bean.title%></h4>
                                </div>
                            </li>
                            <% })
                            } %>
                        </ul>
                        <div class="clearfix"> </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- footer -->
<%include footer.ejs%>
<!-- //footer -->
<!-- //copy-right -->
<!-- for bootstrap working -->
<script src="/front/js/bootstrap.js"></script>
<!-- //for bootstrap working -->
<!-- here stars scrolling icon -->
<script type="text/javascript">
    $(document).ready(function () {
        /*
         var defaults = {
         containerID: 'toTop', // fading element id
         containerHoverID: 'toTopHover', // fading element hover id
         scrollSpeed: 1200,
         easingType: 'linear'
         };
         */

        $().UItoTop({easingType: 'easeOutQuart'});

    });
</script>
<!-- //here ends scrolling icon -->
</body>
</html>