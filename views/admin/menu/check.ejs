Version: 1.0.0
Purchase: http://wrapbootstrap.com
-->

<html xmlns="http://www.w3.org/1999/xhtml">
<!-- Head -->
<head>
    <meta charset="utf-8"/>
    <title>后台管理系统-工器具管理</title>
    <% include ../common/style.ejs %>
    <link href="/stylesheets/bootstrap-select.min.css" rel="stylesheet"/>
    <!--Skin Script: Place this script in head to load scripts for skins and rtl support-->
    <style type="text/css">
        .page-container {
            display: block;
            /* margin-left: 224px; */
            margin-right: 0;
            margin-top: 0;
            min-height: 100%;
            padding: 0;
        }

        input[type=checkbox] {
            opacity: 1;
            position: relative;
            left: 0px;
            z-index: 12;
            width: 18px;
            height: 18px;
            cursor: pointer;
        }

        .black_overlay {
            display: none;
            position: absolute;
            top: 0%;
            left: 0%;
            width: 100%;
            height: 100%;
            background-color: black;
            z-index: 1001;
            -moz-opacity: 0.8;
            opacity: .80;
            filter: alpha(opacity=80);
        }

        .white_content {
            display: none;
            position: absolute;
            top: 10%;
            left: 10%;
            width: 30%;
            height: 50%;
            border: 16px solid lightblue;
            background-color: white;
            z-index: 1002;
            overflow: auto;
        }

        .white_content_small {
            display: none;
            position: absolute;
            top: 20%;
            left: 30%;
            width: 40%;
            height: 50%;
            border: 16px solid lightblue;
            background-color: white;
            z-index: 1002;
            overflow: auto;
        }
    </style>
</head>
<!-- /Head -->
<!-- Body -->
<body>
<!-- Main Container -->
<div class="main-container container-fluid">
    <!-- Page Container -->
    <div class="page-container">
        <!-- Page Body -->
        <div class="page-body">
            <div class="row">
                <div class="col-lg-12 col-sm-12 col-xs-12">
                    <div class="widget">
                        <div class="widget-header bordered-bottom bordered-palegreen">
                            <span class="widget-caption">详情</span>
                        </div>
                        <div class="widget-body">
                            <div>
                                <form class="form-horizontal form-bordered" role="form" method="post"
                                      action="" id="jvForm">
                                    <div class="form-group">
                                        <label for="name" class="col-sm-2 control-label no-padding-right">父级菜单</label>
                                        <div class="col-sm-4">
                                            <input type="text" class="form-control" readonly
                                                   value="<%=bean && '' != bean.parentName ? bean.parentName : '顶级菜单'%>">
                                        </div>
                                        <label for="name" class="col-sm-2 control-label no-padding-right">创建者</label>
                                        <div class="col-sm-4">
                                            <input type="text" class="form-control" readonly
                                                   value="<%=bean ? bean.createUserName : ''%>">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="name" class="col-sm-2 control-label no-padding-right">中文名</label>

                                        <div class="col-sm-4">
                                            <input type="text" class="form-control" readonly value="<%=bean ? bean.zhName : ''%>">
                                        </div>

                                        <label for="name" class="col-sm-2 control-label no-padding-right">创建时间</label>
                                        <div class="col-sm-4">
                                            <input type="text" class="form-control" readonly
                                                   value="<%=bean ? bean.createTime : ''%>">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="name" class="col-sm-2 control-label no-padding-right">英文名</label>

                                        <div class="col-sm-4">
                                            <input type="text" class="form-control" readonly value="<%=bean ? bean.enName : ''%>">
                                        </div>
                                        <label for="name" class="col-sm-2 control-label no-padding-right">更新者</label>
                                        <div class="col-sm-4">
                                            <input type="text" class="form-control" readonly
                                                   value="<%=bean ? bean.updateUserName : ''%>">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="name" class="col-sm-2 control-label no-padding-right">icon</label>

                                        <div class="col-sm-4">
                                            <input type="text" class="form-control" readonly value="<%=bean ? bean.icon : ''%>">
                                        </div>
                                        <label for="name" class="col-sm-2 control-label no-padding-right">排序</label>
                                        <div class="col-sm-4">
                                            <input type="text" class="form-control" readonly
                                                   value="<%=bean ? bean.sort : ''%>">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="name" class="col-sm-2 control-label no-padding-right">url</label>

                                        <div class="col-sm-4">
                                            <input type="text" class="form-control" readonly value="<%=bean ? bean.url : ''%>">
                                        </div>
                                        <label for="name" class="col-sm-2 control-label no-padding-right">更新时间</label>
                                        <div class="col-sm-4">
                                            <input type="text" class="form-control" readonly
                                                   value="<%=bean ? bean.updateTime : ''%>">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="name" class="col-sm-2 control-label no-padding-right">target</label>
                                        <div class="col-sm-4">
                                            <input type="text" class="form-control" readonly value="<%=bean ? bean.target : ''%>">
                                        </div>
                                        <label for="name" class="col-sm-2 control-label no-padding-right">是否显示</label>
                                        <div class="col-sm-4">
                                            <input type="text" class="form-control"
                                                   readonly value="<%=bean && bean.isDisplay == 0 ? '是' : '否'%>">
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- /Page Body -->
    </div>
    <!-- /Page Content -->
</div>
<!-- /Page Container -->
<!-- Main Container -->
</div>
</body>
<!--  /Body -->
</html>
